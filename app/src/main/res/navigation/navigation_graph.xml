        android:name="com.abouttime.blindcafe.presentation.main.my_page.setting.report_list.ReportListFragment"
        android:label="ReportListFragment"
        tools:layout="@layout/fragment_report_list" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.abouttime.blindcafe.presentation.main.my_page.setting.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting">
        <action
            android:id="@+id/action_settingFragment_to_reportListFragment"
            app:destination="@id/reportListFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_settingFragment_to_accountDeleteFragment"
            app:destination="@id/accountDeleteFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_settingFragment_to_confirmDialogFragment"
            app:destination="@id/confirmDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileEditFragment"
        android:name="com.abouttime.blindcafe.presentation.main.my_page.edit.profile.info.ProfileEditFragment"
        android:label="ProfileEditFragment"
        tools:layout="@layout/fragment_profile_edit" >
        <action
            android:id="@+id/action_profileEditFragment_to_locationFragment"
            app:destination="@id/locationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/profileExchangeCompleteFragment"
        android:name="com.abouttime.blindcafe.presentation.profile_exchange.complete.ProfileExchangeCompleteFragment"
        android:label="ProfileExchangeCompleteFragment"
        tools:layout="@layout/fragment_profile_exchagnge_complete" />
    <fragment
        android:id="@+id/exitFragment"
        android:name="com.abouttime.blindcafe.presentation.main.home.exit.ExitFragment"
        android:label="ExitFragment"
        tools:layout="@layout/fragment_exit">
        <argument android:name="isReport"
            app:argType="boolean"
            app:nullable="false" />

        <argument
            android:name="title"
            app:argType="string"
            app:nullable="false" />


    </fragment>
    <fragment
        android:id="@+id/profileExchangeFragment"
        android:name="com.abouttime.blindcafe.presentation.profile_exchange.ProfileExchangeFragment"
        android:label="ProfileExchangeFragment"
        tools:layout="@layout/fragment_profile_exchange">
        <action
            android:id="@+id/action_profileExchangeFragment_to_locationFragment"
            app:destination="@id/locationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profileExchangeFragment_to_profileExchangeCompleteFragment"
            app:destination="@id/profileExchangeCompleteFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_profileExchangeFragment_to_profileDismissFragment"
            app:destination="@id/profileDismissFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <dialog
        android:id="@+id/confirmDialogFragment"
        android:name="com.abouttime.blindcafe.presentation.common.confirm.ConfirmDialogFragment"
        android:label="ConfirmDialogFragment"
        tools:layout="@layout/dialog_fragment_confirm">
        <!-- 꼭 보낼 5 가지 -->
        <argument
            android:name="id"
            app:argType="integer"
            app:nullable="false" />
        <argument
            android:name="title"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="subtitle"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="no"
            app:argType="string" />
        <argument
            android:name="yes"
            app:argType="string" />


        <!-- 채팅방 나가기에 쓰이는 두 가지 -->
        <argument
            android:name="reason"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="matchingId"
            android:defaultValue="0"
            app:argType="integer" />


        <action
            android:id="@+id/action_confirmDialogFragment_to_accountDeleteCompleteFragment"
            app:destination="@id/accountDeleteCompleteFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_confirmDialogFragment_to_exitFragment"
            app:destination="@id/exitFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/matchingFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_confirmDialogFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_confirmDialogFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true" />
    </dialog>
