        android:id="@+id/accountDeleteCompleteFragment"
        android:name="com.abouttime.blindcafe.presentation.main.my_page.setting.account_delete.complete.AccountDeleteCompleteFragment"
        android:label="AccountDeleteCompleteFragment"
        tools:layout="@layout/fragment_account_delete" />
    <fragment
        android:id="@+id/interestEditFragment"
        android:name="com.abouttime.blindcafe.presentation.main.my_page.edit.interest.InterestEditFragment"
        android:label="InterestEditFragment"
        tools:layout="@layout/fragment_interest_edit" />
    <fragment
