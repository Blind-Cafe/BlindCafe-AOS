            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_mainFragment_to_exitFragment"
            app:destination="@id/exitFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_mainFragment_to_profileExchangeFragment"
            app:destination="@id/profileExchangeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_mainFragment_to_interestEditFragment"
            app:destination="@id/interestEditFragment" />
        <action
            android:id="@+id/action_mainFragment_to_confirmDialogFragment"
            app:destination="@id/confirmDialogFragment" />
        <action
            android:id="@+id/action_mainFragment_to_dormancyFragment"
            app:destination="@id/dormancyFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_mainFragment_to_profileImageEditFragment"
            app:destination="@id/profileImageEditFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/ruleFragment"
        android:name="com.abouttime.blindcafe.presentation.onboarding.rule.RuleFragment"
        android:label="RuleFragment"
        tools:layout="@layout/fragment_rule">
        <action
            android:id="@+id/action_ruleFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/agreementFragment"
        android:name="com.abouttime.blindcafe.presentation.onboarding.agreement.AgreementFragment"
        android:label="AgreementFragment"
        tools:layout="@layout/fragment_agreement">
        <action
            android:id="@+id/action_agreementFragment_to_profileSettingFragment"
            app:destination="@id/essentialFirstFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_agreementFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/agreementFragment"
            app:popUpToInclusive="true" />

    </fragment>
    <fragment
        android:id="@+id/essentialFirstFragment"
        android:name="com.abouttime.blindcafe.presentation.onboarding.profile_setting.essential_first.EssentialFirstFragment"
        android:label="ProfileSettingFragment"
        tools:layout="@layout/fragment_essential_first">
        <action
            android:id="@+id/action_profileSettingFragment_to_essentialSecondFragment"
            app:destination="@id/essentialSecondFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/essentialSecondFragment"
        android:name="com.abouttime.blindcafe.presentation.onboarding.profile_setting.essential_second.EssentialSecondFragment"
        android:label="EssentialSecondFragment"
        tools:layout="@layout/fragment_essential_second">
        <action
            android:id="@+id/action_essentialSecondFragment_to_interestFragment"
            app:destination="@id/interestFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

    <fragment
        android:id="@+id/interestFragment"
        android:name="com.abouttime.blindcafe.presentation.onboarding.profile_setting.interest.InterestFragment"
        android:label="InterestFragment"
        tools:layout="@layout/fragment_interest">
        <action
            android:id="@+id/action_interestFragment_to_interestSubFragment"
            app:destination="@id/interestSubFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/matchingFragment"
        android:name="com.abouttime.blindcafe.presentation.chat.ChatFragment"
        android:label="MatchingFragment"
        tools:layout="@layout/fragment_chat">

        <argument
            android:name="matchingId"
            app:argType="integer"
            app:nullable="false" />

        <argument
            android:name="startTime"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="partnerNickname"
            app:argType="string"
            app:nullable="false" />

        <action
            android:id="@+id/action_matchingFragment_to_quitReasonDialogFragment"
            app:destination="@id/quitReasonDialogFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_matchingFragment_to_reportReasonDialogFragment"
            app:destination="@id/reportReasonDialogFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/galleryDialogFragment"
        android:name="com.abouttime.blindcafe.presentation.chat.gallery.GalleryDialogFragment"
        android:label="GalleryDialogFragment"
        tools:layout="@layout/dialog_fragment_gallery" />
    <fragment
        android:id="@+id/coffeeOrderFragment"
        android:name="com.abouttime.blindcafe.presentation.main.home.coffee.CoffeeOrderFragment"
        android:label="CoffeeOrderFragment"
        tools:layout="@layout/fragment_coffee_order">

        <argument
            android:name="matchingId"
            app:argType="integer" />


        <argument
            android:name="startTime"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="partnerNickname"
            app:argType="string"
            app:nullable="true" />

        <action
            android:id="@+id/action_coffeeOrderFragment_to_matchingFragment"
            app:destination="@id/matchingFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/coffeeOrderFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/signinFragment"
        android:name="com.abouttime.blindcafe.presentation.onboarding.signin.SigninFragment"
        android:label="SigninFragment"
        tools:layout="@layout/fragment_signin">
        <action
            android:id="@+id/action_signinFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/agreementFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/interestSubFragment"
        android:name="com.abouttime.blindcafe.presentation.onboarding.profile_setting.interest_detail.InterestSubFragment"
        android:label="InterestSubFragment"
        tools:layout="@layout/fragment_interest_sub">
        <action
            android:id="@+id/action_interestSubFragment_to_signinFragment"
            app:destination="@id/signinFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <dialog
        android:id="@+id/quitReasonDialogFragment"
        android:name="com.abouttime.blindcafe.presentation.chat.quit.QuitReasonDialogFragment"
        android:label="QuitReasonDialogFragment"
        tools:layout="@layout/dialog_fragment_quit_reason">

        <argument
            android:name="matchingId"
            app:argType="integer"
            app:nullable="false" />
        <action
            android:id="@+id/action_quitReasonDialogFragment_to_confirmDialogFragment"
            app:destination="@id/confirmDialogFragment"
            app:popUpTo="@id/quitReasonDialogFragment"
            app:popUpToInclusive="true" />
    </dialog>
    <dialog
        android:id="@+id/reportReasonDialogFragment"
        android:name="com.abouttime.blindcafe.presentation.chat.report.ReportReasonDialogFragment"
        android:label="ReportReasonDialogFragment"
        tools:layout="@layout/dialog_fragment_report_reason">
        <argument
            android:name="matchingId"
            app:argType="integer"
            app:nullable="false" />
        <action
            android:id="@+id/action_reportReasonDialogFragment_to_confirmDialogFragment"
            app:destination="@id/confirmDialogFragment"
            app:popUpTo="@id/reportReasonDialogFragment"
            app:popUpToInclusive="true" />

    </dialog>
    <fragment
        android:id="@+id/accountDeleteFragment"
        android:name="com.abouttime.blindcafe.presentation.main.my_page.setting.account_delete.AccountDeleteFragment"
        android:label="AccountDeleteFragment"
        tools:layout="@layout/fragment_account_delete">
        <action
            android:id="@+id/action_accountDeleteFragment_to_confirmDialogFragment"
            app:destination="@id/confirmDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/locationFragment"
        android:name="com.abouttime.blindcafe.presentation.profile_exchange.location.LocationFragment"
        android:label="LocationFragment"
        tools:layout="@layout/fragment_location" />
    <fragment
        android:id="@+id/profileDismissFragment"
        android:name="com.abouttime.blindcafe.presentation.profile_exchange.dismiss.ProfileDismissFragment"
        android:label="ProfileDismissFragment"
        tools:layout="@layout/fragment_profile_dismiss">
        <action
            android:id="@+id/action_profileDismissFragment_to_confirmDialogFragment"
            app:destination="@id/confirmDialogFragment" />
    </fragment>
    <fragment
