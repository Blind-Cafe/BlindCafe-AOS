        tools:layout="@layout/fragment_dormancy" />
    <fragment
        android:id="@+id/profileImageEditFragment"
        tools:layout="@layout/fragment_profile_image_edit"
        android:name="com.abouttime.blindcafe.presentation.main.my_page.edit.profile.image.ProfileImageEditFragment"
        android:label="ProfileImageEditFragment" />
</navigation>